<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dashboard_navigation"
    app:startDestination="@id/navigation_home">

    <include-dynamic
        android:id="@+id/navigation_home"
        app:graphPackage="${applicationId}.feature_home"
        app:graphResName="home_navigation"
        app:moduleName="feature_home" />

    <include-dynamic
        android:id="@+id/navigation_favorite"
        app:graphPackage="${applicationId}.feature_favorite"
        app:graphResName="favorite_navigation"
        app:moduleName="feature_favorite" />

    <include-dynamic
        android:id="@+id/navigation_settings"
        app:graphPackage="${applicationId}.feature_settings"
        app:graphResName="settings_navigation"
        app:moduleName="feature_settings" />
</navigation>